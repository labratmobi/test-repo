{
  "name": "web-project-template",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --mode production",
    "build:dev": "npm run clean && webpack --mode development",
    "build:watch": "npm run clean && webpack serve --mode development --open",
    "check-code-style": "prettier src/**/*.ts && eslint src/**/*.ts --ext .ts",
    "fix-code-style": "prettier --write src/**/*.ts && eslint src/**/*.ts --ext .ts --fix",
    "test": "jest --silent --bail",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverage=true"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.8.0",
    "@typescript-eslint/parser": "^4.8.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-functional": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-rxjs": "^2.1.3",
    "eslint-plugin-unicorn": "^23.0.0",
    "eslint-webpack-plugin": "^3.1.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.0",
    "html-replace-webpack-plugin": "^2.6.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "27.0.6",
    "prettier": "^2.4.1",
    "rimraf": "^2.6.3",
    "style-loader": "^3.3.1",
    "ts-jest": "27.0.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  },
  "dependencies": {
    "rxjs": "^6.6.7"
  }
}
